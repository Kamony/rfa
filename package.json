{
  "name": "rfa",
  "author": "Jiri Hauser",
  "version": "0.2.1",
  "license": "MIT",
  "homepage": "https://github.com/Kamony/rfa",
  "keywords": ["form", "form-builder", "form-architect", "dynamic-form", "react-forms", "validations", "user", "user-defined-form", "typescript-form", "material-ui-form", "tree"],
